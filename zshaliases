# custom aliases to reload/open zshrc config
alias zshreload=". ~/.zshrc"
alias zshenv=". ~/.zshd/zshenv"

# custom git aliases
alias gl="git pull"
alias gll="git log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit"
alias gs="git status"
alias gp="git push"
alias gco="git checkout "
alias gcm="git commit -m "
alias ga="git add"
alias g="git "
alias gd="git diff "
alias gdmb='git branch --merged | egrep -v " (master|main|dev)" | xargs git branch -d'
alias gdmbForce='git branch | egrep -v " (master|main|dev)" | xargs git branch -D'
alias gb="git branch "
